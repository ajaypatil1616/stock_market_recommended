    "import schedule\n",
    "import time\n",
    "import os\n",
    "\n",
    "def run_notebook():\n",
    "    os.system('jupyter nbconvert --execute --to notebook --inplace 1.ipynb')\n",
    "\n",
    "schedule.every().day.at(\"11:27\").do(run_notebook)\n",
    "\n",
    "while True:\n",
    "    schedule.run_pending()\n",
    "    time.sleep(60)  \n"
   ]
